{
    "tailwindCSS.includeLanguages": {
        "html": "html",
        "javascript": "javascript",
        "typescript": "typescript",
        "javascriptreact": "javascriptreact",
        "typescriptreact": "typescriptreact",
        "css": "css"
    },
    "editor.quickSuggestions": {
        "strings": true
    },
    "tailwindCSS.experimental.configFile": "app/frontend/tailwind.config.js",
    "files.associations": {
        "*.html": "html",
        "*.js": "javascript",
        "*.ts": "typescript",
        "*.jsx": "javascriptreact",
        "*.tsx": "typescriptreact",
        "*.css": "css"
    },
}